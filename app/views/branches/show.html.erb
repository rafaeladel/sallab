<p class="alert alert-block alert-info hide" id="notice">
	<a class="close" data-dismiss="alert" href="#" aria-hidden="true">&times;</a>
	<%= notice %>
</p>
<div class="box border blue">
	<div class="box-title">
		<h4><i class="fa fa-calendar"></i> Branch:</h4>
	</div>
	<div class="box-body">
		 <div class="form-horizontal " action="#">
			<div class="form-group">
				<label class="col-md-3 control-label">Address (English)</label>
				<div class="col-md-9">
					<input type="text" name="regular" class="form-control" value="<%= @branch.address_en %>" disabled>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-3 control-label">Address (Arabic)</label>
				<div class="col-md-9">
					<input type="text" name="regular" class="form-control" value="<%= @branch.address_ar %>" disabled>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-3 control-label">Phone</label>
				<div class="col-md-9">
					<input type="text" name="regular" class="form-control" value="<%= @branch.phone %>" disabled>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-3 control-label">Region</label>
				<div class="col-md-9">
					<input type="text" name="regular" class="form-control" value="<%= @branch.region %>" disabled>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-3 control-label">Latitude</label>
				<div class="col-md-9">
					<input type="text" name="regular" class="form-control" value="<%= @branch.lat %>" disabled>
				</div>
			</div>
			<div class="form-group">
				<label class="col-md-3 control-label">Longitude</label>
				<div class="col-md-9">
					<input type="text" name="regular" class="form-control" value="<%= @branch.long %>" disabled>
				</div>
			</div>
			<div class="form-group text-center">
				<%= link_to 'Edit', edit_branch_path(@branch), :class => "btn btn-warning" %> | 
				<%= link_to 'Back', branches_path ,:class => "btn btn-primary" %>
			</div>																			
		</div>
	</div>
</div>