    <div class="box border primary">
      <div class="box-title">
        <h4><i class="fa fa-money"></i>Branch Info</h4>
      </div>
      <div class="box-body">
        <div class="tabbable header-tabs">
          <ul class="nav nav-tabs">
            <li><a href="#box_tab1" data-toggle="tab"><i class="fa fa-keyboard-o"></i>
              <span class="hidden-inline-mobile">Arabic</span></a></li>
            <li class="active"><a href="#box_tab2" data-toggle="tab"><i class="fa fa-globe"></i>
              <span class="hidden-inline-mobile">English</span></a></li>
          </ul>
          <div class="tab-content">
            <div class="tab-pane fade" id="box_tab1">
              <div class="form-group clearfix">
                <%= f.label :address_ar, class: "col-sm-3 control-label" %>
                <div class="col-sm-9">
                  <%= f.text_area :address_ar, class: "form-control" %>
                </div>
              </div>
            </div>
            <div class="tab-pane fade in active" id="box_tab2">
              <div class="form-group clearfix">
                <%= f.label :address_en, class: "col-sm-3 control-label" %>
                <div class="col-sm-9">
                  <%= f.text_area :address_en, class: "form-control" %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="box border lite">
      <div class="box-title">
        <h4><i class="fa fa-bars"></i>General</h4>
      </div>
      <div class="box-body center">
        <div class="form-group clearfix">
		  <%= f.label :region_id, class: "col-sm-3 control-label" %>
          <div class="col-sm-9">
			<%= f.collection_select(:region_id, Region.all, :id, :title_en), class: "form-control" %>
          </div>
        </div>
        <div class="form-group clearfix">
		  <%= f.label :phone, class: "col-sm-3 control-label" %>
          <div class="col-sm-9">
			<%= f.text_field :phone, class: "control-label" %>
          </div>
        </div>
        <div class="form-group clearfix">
		  <%= f.label :location, "Location", class: "col-sm-3 control-label" %>
          <div class="col-sm-9">
			  <div id="map_canvas" style="width:100%; height:150px"></div>
          </div>
        </div>
		
        <div class="form-group clearfix">
		  <%= f.label :lat, class: "col-sm-3 control-label" %>
          <div class="col-sm-9">
			<%= f.text_field :lat, class: "control-label lat_input" %>
          </div>
        </div>
        <div class="form-group clearfix">
		  <%= f.label :long, class: "col-sm-3 control-label" %>
          <div class="col-sm-9">
			<%= f.text_field :long, class: "control-label long_input" %>
          </div>
        </div>
      </div>
    </div>